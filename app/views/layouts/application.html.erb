<!DOCTYPE html>
<html>
  <head>
    <title>JpjCanineTraining</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="o-global-container">
      <div class="o-global-background">
        <div class="o-banner">
          <img src="<%= asset_pack_path 'media/images/output-onlinepngtools.png' %>" alt="Logo" class="o-banner__image" />
        </div>
        <nav>
          <ul class="c-nav">
            <li class="c-nav__item"><a href="#" class="c-nav__item--link">About</a></li>
            <li class="c-nav__item"><a href="#" class="c-nav__item--link">Services</a></li>
            <li class="c-nav__item"><a href="#" class="c-nav__item--link">Contact</a></li>
          </ul>
        </nav>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
